{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nest-js-backend-microservices/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/nest-js-backend-microservices/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/nest-js-backend-microservices",
  "projects": {
    "nest-js-backend-microservices": {
      "type": "application",
      "root": "apps/nest-js-backend-microservices",
      "entryFile": "main",
      "sourceRoot": "apps/nest-js-backend-microservices/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest-js-backend-microservices/tsconfig.app.json"
      }
    },
    "inventory-service": {
      "type": "application",
      "root": "apps/inventory-service",
      "entryFile": "main",
      "sourceRoot": "apps/inventory-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/inventory-service/tsconfig.app.json"
      }
    },
    "order-service": {
      "type": "application",
      "root": "apps/order-service",
      "entryFile": "main",
      "sourceRoot": "apps/order-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/order-service/tsconfig.app.json"
      }
    },
    "payment-service": {
      "type": "application",
      "root": "apps/payment-service",
      "entryFile": "main",
      "sourceRoot": "apps/payment-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/payment-service/tsconfig.app.json"
      }
    },
    "product-service": {
      "type": "application",
      "root": "apps/product-service",
      "entryFile": "main",
      "sourceRoot": "apps/product-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/product-service/tsconfig.app.json"
      }
    },
    "user-service": {
      "type": "application",
      "root": "apps/user-service",
      "entryFile": "main",
      "sourceRoot": "apps/user-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-service/tsconfig.app.json"
      }
    }
  }
}